{
  "pages": [
    "pages/index/index",
    "pages/login/index",
    "pages/role-selection/index",
    "pages/mistakes/list/index",
    "pages/mistakes/detail/index",
    "pages/mistakes/add/index",
    "pages/mistakes/today-review/index",
    "pages/mistakes/review/index",
    "pages/review-recommendations/index",
    "pages/learning/index/index",
    "pages/learning/detail/index",
    "pages/analysis/report/index",
    "pages/analysis/progress/index",
    "pages/profile/index/index",
    "pages/profile/edit/index",
    "pages/profile/settings/index",
    "pages/profile/change-password/index",
    "pages/profile/help/index"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#1890ff",
    "navigationBarTitleText": "五好伴学",
    "navigationBarTextStyle": "white",
    "backgroundColor": "#f5f5f5",
    "enablePullDownRefresh": true,
    "onReachBottomDistance": 50,
    "backgroundColorTop": "#ffffff",
    "backgroundColorBottom": "#ffffff",
    "restartStrategy": "homePage"
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#1890ff",
    "backgroundColor": "#ffffff",
    "borderStyle": "black",
    "position": "bottom",
    "custom": false,
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "assets/icons/home.png",
        "selectedIconPath": "assets/icons/home-active.png"
      },
      {
        "pagePath": "pages/mistakes/list/index",
        "text": "错题本",
        "iconPath": "assets/icons/homework.png",
        "selectedIconPath": "assets/icons/homework-active.png"
      },
      {
        "pagePath": "pages/analysis/report/index",
        "text": "学习报告",
        "iconPath": "assets/icons/report.png",
        "selectedIconPath": "assets/icons/report-active.png"
      },
      {
        "pagePath": "pages/profile/index/index",
        "text": "我的",
        "iconPath": "assets/icons/profile.png",
        "selectedIconPath": "assets/icons/profile-active.png"
      }
    ]
  },
  "networkTimeout": {
    "request": 10000,
    "downloadFile": 10000,
    "uploadFile": 10000,
    "connectSocket": 10000
  },
  "requiredBackgroundModes": ["audio", "backgroundFetch"],
  "preloadRule": {
    "pages/learning/index/index": {
      "network": "all",
      "packages": ["learning"]
    }
  },
  "entryPagePath": "pages/index/index",
  "debug": true,
  "functionalPages": false,
  "subpackages": [
    {
      "root": "subpackages/learning",
      "name": "learning",
      "pages": ["voice/index", "image/index", "history/index"]
    },
    {
      "root": "subpackages/charts",
      "name": "charts",
      "pages": ["pages/knowledge-graph/index"]
    }
  ],
  "workers": "workers",
  "style": "v2",
  "useExtendedLib": {
    "kbone": true,
    "weui": true
  },
  "entranceDeclare": {
    "locationMessage": {
      "path": "pages/learning/index/index",
      "query": "type=location"
    }
  },
  "darkmode": false,
  "lazyCodeLoading": "requiredComponents",
  "singlePage": {
    "navigationBarFit": "squeezed"
  },
  "serviceProviderTicket": "",
  "embeddedAppIdList": [],
  "halfPage": {
    "firstPageNavigationStyle": "custom"
  },

  "enablePassiveEvent": {
    "touchstart": true,
    "touchmove": true
  },
  "usingComponents": {
    "van-action-sheet": "@vant/weapp/action-sheet/index",
    "van-button": "@vant/weapp/button/index",
    "van-cell": "@vant/weapp/cell/index",
    "van-cell-group": "@vant/weapp/cell-group/index",
    "van-icon": "@vant/weapp/icon/index",
    "van-image": "@vant/weapp/image/index",
    "van-popup": "@vant/weapp/popup/index",
    "van-toast": "@vant/weapp/toast/index",
    "van-dialog": "@vant/weapp/dialog/index",
    "van-field": "@vant/weapp/field/index",
    "van-loading": "@vant/weapp/loading/index",
    "van-tag": "@vant/weapp/tag/index",
    "van-empty": "@vant/weapp/empty/index"
  }
}
